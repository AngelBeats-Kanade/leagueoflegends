<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:views="clr-namespace:Lol.Clash.UI.Views"
                    xmlns:ctrls="clr-namespace:Lol.LayoutSupport.Controls;assembly=Lol.LayoutSupport"
                    xmlns:units="clr-namespace:Lol.Clash.UI.Units">
	<Style TargetType="{x:Type views:WinningTeam}">
        <Setter Property="Template">
            <Setter.Value>
				<ControlTemplate TargetType="{x:Type views:WinningTeam}">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <StackPanel>
                            <Grid Margin="48 16 34 0">
                                <units:Title />
                                <ctrls:RiotTextBox MinWidth="220" Height="30" HorizontalAlignment="Right" VerticalAlignment="Top" Tag="Temp" Margin="0 60 0 0"/>
                            </Grid>
                        </StackPanel>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>