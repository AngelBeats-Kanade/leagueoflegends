<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:units="clr-namespace:Lol.Profile.UI.Units">
    <Style TargetType="{x:Type units:RecentChampion}">
        <Setter Property="Margin" Value="16 0 0 0"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type units:RecentChampion}">
                    <StackPanel>
                        <units:PlayChampListBox Height="164" Background="Blue" Margin="0 48 0 0"/>
                        <TextBlock Margin="0 16 0 0" Text="플레이한 게임 중 비율" Foreground="#A09B8C" HorizontalAlignment="Center"/>
                    </StackPanel>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>