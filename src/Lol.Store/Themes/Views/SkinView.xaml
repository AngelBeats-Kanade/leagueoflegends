<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:views="clr-namespace:Lol.Store.UI.Views"
                    xmlns:commons="clr-namespace:Lol.Store.UI.Commons"
					xmlns:units="clr-namespace:Lol.Store.UI.Units"
                    xmlns:ctrls="clr-namespace:Lol.LayoutSupport.Controls;assembly=Lol.LayoutSupport">

    <Style TargetType="{x:Type views:SkinView}">
		<Setter Property="Template">
			<Setter.Value>
                <ControlTemplate TargetType="{x:Type views:SkinView}">
                    <Grid Margin="0">
                    </Grid>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
</ResourceDictionary>